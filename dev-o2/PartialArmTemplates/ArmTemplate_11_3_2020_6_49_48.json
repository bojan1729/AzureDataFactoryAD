											"name": "LastName",
											"type": "String"
										},
										"sink": {
											"name": "LastName",
											"type": "String"
										}
									},
									{
										"source": {
											"name": "deptName",
											"type": "String"
										},
										"sink": {
											"name": "deptName",
											"type": "String"
										}
									},
									{
										"source": {
											"name": "Revshare",
											"type": "Decimal"
										},
										"sink": {
											"name": "Revshare",
											"type": "Double"
										}
									},
									{
